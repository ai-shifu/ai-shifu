{
  "presets": [
    ["@babel/preset-env", { "targets": { "node": "current" } }],
    "@babel/preset-react",
    ["@babel/preset-typescript", { "allowNamespaces": true, "isTSX": true, "allExtensions": true }]
  ],
  "plugins": [
    "@babel/plugin-transform-typescript",
    "@babel/plugin-transform-class-properties"
  ]
}
