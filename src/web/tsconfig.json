{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": true,
    "jsx": "react-jsx",
    "checkJs": false,
    "noImplicitAny": false,
    "noEmitOnError": false,
    "suppressImplicitAnyIndexErrors": true,
    "skipDefaultLibCheck": true,
    "transpileOnly": true,
    "baseUrl": "./src",
    "paths": {
      "@Assets": [
        "./Assets"
      ],
      "@Components": [
        "./Components"
      ],
      "@pages": [
        "./pages"
      ],
      "@assets": [
        "./assets"
      ],
      "@images": [
        "./assets/img"
      ],
      "@icons": [
        "./assets/icons"
      ],
      "@constants": [
        "./constants"
      ],
      "@Service": [
        "./Service"
      ],
      "@Utils": [
        "./Utils"
      ],
      "@stores": [
        "./stores"
      ],
      "@Api": [
        "./Api"
      ],
      "@/*": [
        "./"
      ],
      "components/*": [
        "./components/*"
      ],
      "@ShiNiang": [
        "./ShiNiang"
      ],
      "utils/*": [
        "./utils/*"
      ],
      "Assets/*": [
        "./Assets/*"
      ],
      "Components/*": [
        "./Components/*"
      ],
      "constants/*": [
        "./constants/*"
      ],
      "Service/*": [
        "./Service/*"
      ],
      "Utils/*": [
        "./Utils/*"
      ],
      "stores/*": [
        "./stores/*"
      ],
      "Api/*": [
        "./Api/*"
      ],
      "ShiNiang/*": [
        "./ShiNiang/*"
      ]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}